                              </GridPane>
                              <VBox layoutX="209.0" layoutY="121.0" spacing="5.0">
                                 <children>
                                    <Label text="Login: " />
                                    <TextField fx:id="loginField" disable="true" editable="false" prefHeight="25.0" prefWidth="100.0" />
                                 </children>
                              </VBox>
                              <Button alignment="CENTER" layoutX="209.0" layoutY="202.0" mnemonicParsing="false" onAction="#onLogOut" prefHeight="45.0" prefWidth="150.0" text="Tancar sessió">
